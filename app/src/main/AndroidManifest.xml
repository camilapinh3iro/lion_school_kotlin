<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.Lion_shool"
        tools:targetApi="31" >
        <activity
            android:name=".gui.SelectedStudentActivity"
            android:exported="false"
            android:label="@string/title_activity_selected_student"
            android:theme="@style/Theme.Lion_shool" />
        <activity
            android:name=".gui.SelectedCourse"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Lion_shool" />
        <activity
            android:name=".gui.ContactUsActivity"
            android:exported="false"
            android:label="@string/title_activity_contact_us"
            android:theme="@style/Theme.Lion_shool" />
        <activity
            android:name=".gui.SelectCourse"
            android:exported="false"
            android:label="@string/title_activity_select_course"
            android:theme="@style/Theme.Lion_shool" />
        <activity
            android:name=".gui.MainActivity"
            android:exported="true"
            android:label="@string/title_activity_selected_course"
            android:theme="@style/Theme.Lion_shool" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>